{
  "name": "git-cheatsheet",
  "version": "1.0.0",
  "description": "",
  "main": "server/index.js",
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      "server/views/*",
      "build/*",
      "coverage/*"
    ]
  },
  "scripts": {
    "lint": "eslint src/**/*",
    "test": "jest && codecov",
    "test:dev": "jest --logHeapUsage --runInBand --watchAll",
    "build": "webpack --config webpack.prod.js && babel server --out-dir build/server --copy-files",
    "build:dev": "webpack --config webpack.dev.js && babel-node server",
    "start:server": "nodemon --exec npm run build:dev",
    "start:client": "webpack-dev-server --config webpack.dev.js",
    "start": "npm run build && node build/server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
